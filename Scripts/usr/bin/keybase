CLI_IMAGE="kristianfoss/programs-keybase:alpine-client"

CLI_NAME="keybase-client-cli"

/usr/local/bin/ensure-docker-cli "${CLI_NAME}" "${CLI_IMAGE}" "-v ${HOME}:/tmp/home${HOME}  -e HOME=/tmp/home${HOME}"

exec /usr/bin/docker exec -it --workdir "/tmp/home${PWD}" ${CLI_NAME} /usr/bin/keybase "${@}"