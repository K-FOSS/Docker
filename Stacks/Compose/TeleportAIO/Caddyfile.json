{
  "admin": {
    "disabled": true
  },
  "apps": {
    "http": {
      "http_port": 80,
      "https_port": 443,
      "servers": {
        "example.com": {
          "listen": [":80"],
          "automatic_https": {
            "disable": true
          },

          "routes": [
            {
              "match": [
                {
                  "host": ["teleport.kristianjones.dev"]
                }
              ],
              "handle": [
                {
                  "handler": "reverse_proxy",
                  "upstreams": [
                    {
                      "dial": "Teleport:3080"
                    }
                  ]
                }
              ]
            },
            {
              "match": [
                {
                  "host": ["proxy.kristianjones.dev"]
                }
              ],
              "handle": [
                {
                  "handler": "reverse_proxy",
                  "upstreams": [
                    {
                      "dial": "Teleport:3023"
                    }
                  ]
                }
              ]
            },
            {
              "match": [
                {
                  "host": ["ssh.kristianjones.dev"]
                }
              ],
              "handle": [
                {
                  "handler": "subroute",
                  "routes": [
                    {
                      "handle": [
                        {
                          "handler": "reverse_proxy",
                          "upstreams": [
                            {
                              "dial": "Teleport:3023"
                            }
                          ]
                        }
                      ]
                    }
                  ]
                }
              ]
            },
            {
              "match": [
                {
                  "host": ["tunnel.kristianjones.dev"]
                }
              ],
              "handle": [
                {
                  "handler": "reverse_proxy",
                  "upstreams": [
                    {
                      "dial": "Teleport:3024"
                    }
                  ]
                }
              ]
            },
            {
              "match": [
                {
                  "host": ["auth.kristianjones.dev"]
                }
              ],
              "handle": [
                {
                  "handler": "reverse_proxy",
                  "upstreams": [
                    {
                      "dial": "Teleport:3025"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
    }
  }
}
