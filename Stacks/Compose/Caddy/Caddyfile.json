{
  "admin": {
    "disabled": true
  },
  "apps": {
    "http": {
      "http_port": 8080,
      "https_port": 8443,
      "servers": {
        "example.com": {
          "listen": [":8080"],

          "automatic_https": {
            "disable": true
          },

          "routes": [
            {
              "handle": [
                {
                  "handler": "static_response",
                  "status_code": "200",
                  "body": "Hello World",
                  "close": true
                }
              ]
            }
          ]
        }
      }
    }
  }
}
